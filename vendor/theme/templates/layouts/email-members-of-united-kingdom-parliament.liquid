{% comment %} Description: A page that allows members to email custom targets. {% endcomment %}
{% comment %} Primary layout: true {% endcomment %}

{% include 'Full Width Header'  %}
<main class="container email-uk-parliament">
  <section class="body-text text-top">
    <div class="one-column">
      {{ content }}
      {% unless link_list == blank %}
        <hr class="stubby-hr" />
        <h3>{{ 'page.more_info' | t }}</h3>
        {% include 'Links' %}
      {% endunless %}
    </div>
  </section>

  <section class="alternate-bg">
    <div class="one-column">
      {% include 'Email', type: 'ukparliament' %}
    </div>
  </section>
</main>

{% include 'Simple Footer' %}


