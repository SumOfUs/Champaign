window.ChampaignSettings = {};

<% if Settings.external_js_path.present? %>
window.ChampaignSettings.external_js_path = '<%= Settings.external_js_path %>';
<% end %>

<% if Rails.env.production? %>
  Object.assign(window.ChampaignSettings, {
    airbrake: {
      projectId: "<%= Settings.airbrake_project_id %>",
      projectKey: "<%= Settings.airbrake_api_key %>",
    },
  });
<% end %>
