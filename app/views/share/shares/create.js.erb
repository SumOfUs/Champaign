<%- if @share.errors.empty? %>
    <%- if @share.share_progress? %>
        $("table.shares-editor__existing__shareprogress tbody").append('<%= escape_javascript(render @share, page: @page) %>');
    <% else %>
        $("table.shares-editor__existing__champaign tbody").append('<%= escape_javascript(render @share, page: @page) %>');
    <% end %>
<% end %>

$("#share-url-updater").
  html('<%= escape_javascript(render 'share/update_urls_form') %>');
